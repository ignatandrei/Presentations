{
  "servers": {
    "github-mcp": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp"
    },
    "time": {
        "command": "docker",
        "args": ["run", "-i", "--rm", "mcp/time"]
      },
    
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "${workspaceFolder}",
        "D:\\test",
        "D:\\src"
      ]
    }
  }
  
}
