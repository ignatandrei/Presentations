{
  "servers": {
    "github-mcp": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp"
    },
    "time": {
      "command": "docker",
      "args": ["run", "-i", "--rm", "mcp/time"],
      "type": "stdio"
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem"],
      "type": "stdio"
    },
    "MCPArray2Any": {
      "type": "stdio",
      "command": "dnx",
      "args": ["MCPArray2Any@10.2025.1125.656", "--yes"],
      "env": {}
    },
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"],
      "type": "stdio"
    }
  },
  "inputs": []
}
